<database>
	<queries>
		<query name="test_ID">
			<id>t.ID</id>
			<label>concat( t.apellido, ', ', t.nombre )</label>
			<from>test</from>
			<alias>t</alias>
			<order_by>t.apellido</order_by>
		</query>
	</queries>
</database>
