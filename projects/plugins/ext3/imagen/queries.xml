<queries>

		<query name="foto_ID">
			<id>foto.ID</id>
			<label>concat(foto.dirname,'/',foto.basename)</label>
			<from>foto</from>
			<order_by>foto.fecha,foto.fotografo_alias,foto.foto</order_by>
		</query>

</queries>
